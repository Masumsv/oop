#include <iostream>
#include <string>

using namespace std;

class person {
protected:
    string f_name = "f name ";
    string l_name = "l name ";

public:
    virtual string GetName() {
        return f_name + l_name;
    }
};

int main() {
    person p;
    cout << p.GetName() << endl;
    return 0;
}
